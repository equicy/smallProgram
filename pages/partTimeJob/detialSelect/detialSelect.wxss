.filterContianer{
  display: flex;
  flex-direction: column;
  width: 100%;
  box-sizing: border-box;
}
.area{
  margin: 20rpx;
  width: 100%;
}
.areaTitle{
  font-size: 24rpx;
  color: #333333;
  /* display: flex;
  flex-direction: row; */
}
.areaTitleFirst{
  border-bottom: 7rpx solid #eeeeee;
}
.areaContext{
  font-size: 22rpx;
  color: #b1b1b1;
  margin:28rpx 65rpx 0 20rpx;
  display: flex;
  flex-wrap: wrap;
}
.areaContextElement{
  width: 121rpx;
  height: 34rpx;
  line-height: 34rpx;
  margin-left: 30rpx;
  border-radius: 17rpx;
  border: 1px solid #b1b1b1;
  text-align: center;
  margin-bottom: 22rpx;
}
.filterSubmit{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 30rpx;
  color: #4cd94c;
  display: flex;
  flex-direction: row;
  border-top: 1px solid #eeeeee;
}
.reset{
  width: 50%;
  height: 100%;
}
.submit{
  width:50%;
  height: 100%;
  background-color: #4cd94c;
  color: #fff;
}