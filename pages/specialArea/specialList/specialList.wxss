.spcialList{
  width: 100%;
  height: 250rpx;
  border-radius: 20rpx;
  box-shadow: 5px 5px 20px 0 rgba(0, 0, 0, 0.4);
  margin-top: 30rpx;
  box-sizing: border-box;
  position: relative;
}
.spcialImg{
  width: 100%;
  height: 100%;
}
.spcialImg image{
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
